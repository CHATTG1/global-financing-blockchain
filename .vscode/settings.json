{
    "emmet.showSuggestionsAsSnippets": true,
    "eslint.provideLintTask": true,
    "gitProjectManager.openInNewWindow": true,
    "githubPullRequests.logLevel": "off",
    "gitlens.blame.heatmap.location": "left",
    "workbench.editor.revealIfOpen": true,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.quickSuggestionsDelay": 5,
    "files.autoSave": "afterDelay",
    "workbench.commandPalette.preserveInput": true,
    "workbench.quickOpen.preserveInput": true,
    "workbench.colorTheme": "Material Theme Ocean",
    "debug.enableAllHovers": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.enableBell": true,
    "editor.fontSize": 10,
    "zenMode.restore": true,
    "explorer.openEditors.visible": 4,
    "search.showLineNumbers": true,
    "search.useLegacySearch": true,
    "debug.inlineValues": true,
    "debug.internalConsoleOptions": "openOnSessionStart",
    "debug.showInStatusBar": "always",
    "terminal.integrated.cursorStyle": "line",
    "gitlens.outputLevel": "verbose",
    "docker.defaultRegistryPath": "registry.ng.bluemix.net/ascendant-rex"
}